<div class="top-products">
	<div class="container">
		<h3>Top Products</h3>
		<div class="sap_tab">
			<div id="horizontalTab">
				
					<li class="resp-tab-item"><span>Men's</span></li>
					<li class="resp-tab-item"><span>Women</span></li>
                <hr>
				</ul>
				<div class="clearfix"> </div>
				<div class="resp-tabs-container">
					<div class="tab-1 resp-tab-content">
						<% for(var i = 0; i < prod.length;i++) { %>
						<% if(prod[i].category =="Men") { %>
						<div class="col-md-3 top-product-grids tp4">
							<a href="/products/<%= prod[i].category %>/<%= prod[i].title %>"><div class="product-img">
								<img src="/product_images/<%= prod[i]._id %>/<%= prod[i].image %>" alt="" />
								<div class="p-mask">
							<a href="/cart/add/<%= prod[i].title %>">
									<button type="submit" class="w3ls-cart pw3ls-cart"><i class="fa fa-cart-plus" aria-hidden="true"></i> Add to cart</button></a>
								</div>
							</div></a>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star gray-star" aria-hidden="true"></i>
							<i class="fa fa-star gray-star" aria-hidden="true"></i>
							<% if(prod[i].discount) { %>
							<h4><%= prod[i].title %></h4>
							
							<div style="margin-bottom:30px" class="p-mask"><span><h5><strike >$<%= prod[i].price %></strike>&nbsp; &nbsp; $<%= prod[i].discount %></h5><h5 style="float:right"></h5></span></div>
							
							<% } else { %>
							<h4><%= prod[i].title %></h4>
							<h5 style="margin-bottom:30px">$<%= prod[i].price %>
							<% } %>
							</h5>
						</div>
						<% } %>
						<% } %>

						<div class="clearfix"></div>
</div>
					</div>
					<div class="tab-1 resp-tab-content">




						<div class="clearfix"></div>
						<div class="top-products-set2">
						<% for(var i = 0; i < prod.length;i++) { %>
						<% if(prod[i].category =="Women") { %>
						<div class="col-md-3 top-product-grids tp4">
							<a href="/products/<%= prod[i].category %>/<%= prod[i].title %>"><div class="product-img">
								<img src="/product_images/<%= prod[i]._id %>/<%= prod[i].image %>" alt="" />
								<div class="p-mask">
								
								<a href="/cart/add/<%= prod[i].title %>">
									<button type="submit" class="w3ls-cart pw3ls-cart"><i class="fa fa-cart-plus" aria-hidden="true"></i> Add to cart</button></a>
								
								</div>
							</div></a>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star yellow-star" aria-hidden="true"></i>
							<i class="fa fa-star gray-star" aria-hidden="true"></i>
							<i class="fa fa-star gray-star" aria-hidden="true"></i>
							<% if(prod[i].discount) { %>
							<h4><%= prod[i].title %></h4>
							
							<div style="margin-bottom:30px" class="p-mask"><span><h5><strike >$<%= prod[i].price %></strike>&nbsp; &nbsp; $<%= prod[i].discount %></h5><h5 style="float:right"></h5></span></div>
							
							<% } else { %>
							<h4><%= prod[i].title %></h4>
							<h5 style="margin-bottom:30px">$<%= prod[i].price %>
							
							</h5>
							<% } %>
						</div>
						<% } %>
						<% } %>

						<div class="clearfix"></div>
						</div>
					</div>


						</div>
					</div>


						</div>
					</div>

